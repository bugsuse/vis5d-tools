PROCESS HELP=*
REFGBL ($MAPFIL,$RESPOND,$GDFILE,$GFUNC,$GAREA,$DELTAX,$LEVELS)  
REFGBL ($GVCORD,$GDATTIM,$GNAME)
PARM	NAME=GDFILE	TYPE=(STRING,72)  DEFAULT=@$GDFILE
PARM	NAME=GFUNC	TYPE=(STRING,72)  DEFAULT=@$GFUNC
PARM	NAME=GAREA	TYPE=(STRING,48)  DEFAULT=@$GAREA
PARM	NAME=DELTAX	TYPE=(STRING,8)   DEFAULT=@$DELTAX
PARM	NAME=LEVELS     TYPE=(STRING,32)  DEFAULT=@$LEVELS
PARM	NAME=GVCORD     TYPE=(STRING,4)   DEFAULT=@$GVCORD
PARM    NAME=GDATTIM    TYPE=(STRING,48)  DEFAULT=@$GDATTIM
PARM    NAME=GNAME      TYPE=(STRING,4)   DEFAULT=@$GNAME
END-PROC
        PROGRAM GDCFIL
.LEVEL1
.VAR GDFILE
*GEMPAK file name
.VAR GFUNC
*Name of file with functions
.VAR GAREA
*Lat/lon bounds of output grid
.VAR DELTAX
*VIS5D grid spacing (deg)
.VAR LEVELS
*Z bot - Z top - dZ
.VAR GVCORD
*Input vertical coordinate
.VAR GDATTIM
*Grid date/time
.VAR GNAME
*VIS5D output file number
.LEVEL2
.VAR GDFILE
.INCLUDE GEMHLP:GDFILE.HL2
.VAR GFUNC
.INCLUDE GEMHLP:GFUNC.HL2
.VAR GAREA
.INCLUDE GEMHLP:GAREA.HL2
.VAR DELTAX
.INCLUDE GEMHLP:DELTAX.HL2
.VAR LEVELS
.INCLUDE GEMHLP:LEVELS.HL2
.VAR GVCORD
.INCLUDE GEMHLP:GVCORD.HL2
.VAR GDATTIM
.INCLUDE GEMHLP:GDATTIM.HL2
.HELP
.INCLUDE GEMHLP:GEMVIS.HLP
